{
  "name": "@code-shaper/nextjs",
  "description": "Next.js plugin",
  "version": "1.1.4",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/index.js",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "rimraf dist && run-s _bundle",
    "clean": "rimraf --glob .turbo node_modules dist coverage ./**/test-output",
    "format": "prettier --list-different '**/*.{js,jsx,ts,tsx,json,md}'",
    "format:fix": "npm run format -- --write",
    "lint": "eslint '**/*.{js,jsx,ts,tsx}'",
    "lint:fix": "npm run lint -- --fix",
    "test": "echo No tests in this package",
    "_bundle": "tsc --project tsconfig.build.json"
  },
  "dependencies": {
    "@code-shaper/nextjs-generators": "^1.*",
    "@code-shaper/react-generators": "^1.*",
    "@code-shaper/repo-generators": "^1.*",
    "@code-shaper/shaper-utils": "^1.*"
  },
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/node": "^18.17.17",
    "eslint": "^8.49.0",
    "eslint-config-custom": "*",
    "jest": "^29.7.0",
    "jest-config-custom": "*",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2",
    "typescript-config-custom": "*"
  },
  "shaper": {}
}
